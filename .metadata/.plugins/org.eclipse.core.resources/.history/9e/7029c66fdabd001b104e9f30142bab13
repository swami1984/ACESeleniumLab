package StartJava;

public class InterviewProgr {

	public static void main(String[] args) {
		
		String string = "swamy";
		char chars[] = string.toCharArray();
		int count =0;
		
		for(int i=0;i<chars.length;i++) {
			if(string.charAt(i)!= ' ') {
				count++;
			}
		}
		
		System.out.println("total no of chars :"+count);
	}

}
