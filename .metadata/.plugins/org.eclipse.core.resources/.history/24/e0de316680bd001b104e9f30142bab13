package StartJava;

public class javatraining {

	public static void main(String[] args) {
		
		String string = "swamy";
		
		char chars[] = string.toCharArray();
		
		for(int i=1;i<chars.length;i++) {
			int count = 1;
			
			for(int j=i+1;j<chars.length;j++) {
				if(chars[i]==chars[j]&&chars[i]!=' ') {
				count++;	
				chars[j]=0;
				}
				
			}
			
			if(count>1&&chars[i]!=' ') {
				System.out.println(chars[i]);
			}
			
		}
		
		
	}

}
