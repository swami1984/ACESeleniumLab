package practicejava;

public class SingleArray {

	public static void main(String[] args) {
		
	int i[] = new int[4];
		
	i[0]=10;
	i[1]=20;
	i[2]=30;
	i[3]=40;

	System.out.println(i[0]);
	
	for(int a=0;a<i.length;a++) {
		
		System.out.println(i[a]);
	}
		//whilie loop 
	System.out.println("**********************");
	int k =0;
	
	while(k<i.length) {
		System.out.println(k[i]);
		k=k+1;
		
	}
		
	
	
	
	

	}

}
