		//aa.addFirst(600);
		//aa.addLast(900);
		
		System.out.println(aa.get(1));
		
		System.out.println("contents of hashmap"+aa);
		
		//print all values 
		//for each loop
		
		System.out.println("for each loop");
		for(Integer ia:aa) {
			System.out.println(ia);
		}
		
		//forloop 
		System.out.println("for loop");
		for(int i=0;i<aa.size();i++) {
			System.out.println(aa.get(i));
		}
		
		
		//
		

		
		

	}

